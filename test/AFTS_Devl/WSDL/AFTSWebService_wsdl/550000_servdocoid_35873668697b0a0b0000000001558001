	<xs:complexType name="TransferStatusType">
		<xs:sequence>
			<xs:element name="Status" type="tns:StatusType"/>
			<xs:element minOccurs="0" name="ErrorCode" type="tns:ErrorCodeType"/>
			<xs:element minOccurs="0" name="ErrorMessage" type="xs:string"/>
			<xs:element minOccurs="0" name="ErrorType" type="tns:ErrorEnumType"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:simpleType name="StatusType">
		<xs:annotation>
			<xs:documentation> Defines the status of document transfer.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Successful"/>
			<xs:enumeration value="Error"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="ErrorEnumType">
		<xs:annotation>
			<xs:documentation> Defines the type of error occurred in the system.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="PAYLOAD"/>
			<xs:enumeration value="SYSTEM"/>
		</xs:restriction>
	</xs:simpleType>
	
